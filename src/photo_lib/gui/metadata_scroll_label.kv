#:kivy 2.1.0


<MetadataScrollLabel>:
    lbl: lab
    effect_cls: "ScrollEffect"
    always_overscroll: False
    # smooth_scroll_end: 5
    smooth_scroll_end: 0

    Label:
        id: lab
        text: self.parent.text
        font_size: 18
        text_size: None, None
        size_hint_x: None
        size_hint_y: None
        width: self.texture_size[0]
        height: self.texture_size[1]
